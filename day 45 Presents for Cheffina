import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
    public static void main (String[] args) throws java.lang.Exception
    {
        Scanner scanner = new Scanner(System.in);
        int T = scanner.nextInt(); 
        while (T-- > 0) {
            int N = scanner.nextInt(); 
            int minimumCoins = N - N / 5; 
            System.out.println(minimumCoins);
        }
        scanner.close();
    }
}
