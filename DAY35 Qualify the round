import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt(); 
		
		for(int i = 0; i < T; i++) {
			int X = sc.nextInt(); 
			int A = sc.nextInt();
			int B = sc.nextInt(); 
			int totalPoints = A + 2 * B; 
			if(totalPoints >= X) {
				System.out.println("Qualify");
			} else {
				System.out.println("NotQualify");
			}
		}
		
		sc.close();
	}
}
